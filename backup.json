{
	"manifest_version": 2,
	"name": "Chat Bot",
	"description": "Open a chat Plugin on webpage.",
	"version": "0.1",
	"background": {
		"scripts": ["js/background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_title": "Chat Bot"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}